---
export async function getStaticPaths() {
  const abouts = await Astro.glob("@content/about/about-us.md");

  return abouts.map((about) => ({
    params: {
      slug: about.frontmatter.slug,
    },
    props: {
      about,
    },
  }));
}

const { Content } = Astro.props.post;
---

<article>
  <Content />
</article>
