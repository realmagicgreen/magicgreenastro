{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
      "@js/*": ["src/js/*"],
      "@svgs/*": ["public/assets/svgs/*"],
      "@p/*": ["public/assets/p/*"]
    }
  }
}
